ZSH_CACHE_DIR=$HOME/.oh-my-zsh-cache
if [[ ! -d $ZSH_CACHE_DIR ]]; then
  mkdir $ZSH_CACHE_DIR
fi

alias lsblkf='lsblk -fap'
alias rsync='rsync -avzh --progress'
alias du='du -h --max-depth=1 --apparent-size'

function mount {
  if [ "$1" = "sudo" ]; then
    shift
    sudo /usr/bin/mount "$@"
  else
    /usr/bin/mount "$@"
  fi
  pkill -RTMIN+14 i3blocks
}

function umount() {
  if [ "$1" = "sudo" ]; then
    shift
    sudo /usr/bin/umount "$@"
  else
    /usr/bin/umount "$@"
  fi
  pkill -RTMIN+14 i3blocks
}

function axeldownload() {
  axel -v -a -n $1 $2 }

function up() {
  times=$1
  while [ "$times" -gt "0" ]; do
    cd ..
    times=$(($times - 1))
  done
}


